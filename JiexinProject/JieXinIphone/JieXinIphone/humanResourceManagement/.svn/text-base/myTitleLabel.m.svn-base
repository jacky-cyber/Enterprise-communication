#import "myTitleLabel.h"

@implementation myTitleLabel

-(id)initTitleLabel:(NSString*)_name rect:(CGRect)_rect fontSize:(int)_size fontColor:(UIColor*)_color{
    if(self=[super initWithFrame:_rect]){
        self.text=_name;
        self.backgroundColor=[UIColor clearColor];
        if(_color){
            self.textColor=_color;
        }
        self.textAlignment=NSTextAlignmentCenter;
        self.font=[UIFont systemFontOfSize:_size];
        
    }
    return self;
}


@end
